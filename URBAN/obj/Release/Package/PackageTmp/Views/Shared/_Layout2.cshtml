<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ventas Pos</title>
    @*@Styles.Render("~/Content/css")*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/MyMainLayOut.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link rel="icon" type="image/png" href="../Images/POS.jpg" />
    <link href="Content/style.css" rel='stylesheet' type='text/css' /><!-- custom css -->
    <style>
        body {
            font-size: 12px;
            background-color: darkturquoise;
        }
    </style>
</head>
<body>
    <br />
    <header>
        <nav class="navbar-form navbar-right">
            <div class="navbar navbar-inverse navbar-fixed-top col-md-12">
                <div class="container2">
                    <div class="navbar-header col-md-12">
                        <button type="button" class="navbar-toggle pull-right" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav">
                            <li>@Html.ActionLink("Referencias", "Index", "Referencias")</li>
                            <li>@Html.ActionLink("Ventas", "Index", "Ventas")</li>
                            <li>@Html.ActionLink("Salir", "CerrarSession", "Loguin")</li>
                        </ul>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <br />
    <br />
    <div class="container body-content">
        @RenderBody()
        <input type="hidden" id="hdfOculto" value="@Url.Content("~")" />
        <footer>
            <div class="row">
                <div class="col-md-12 copyright">
                    <div class="cpy-right text-center py-3">
                        <br />
                        <div class="cpy-right text-center py-3">
                            <p class="">

                                <span class="text-center text-info">Año - @DateTime.Now.Year | Desarrollado por </span>
                                <span class="text-center text-info"><a href="https://www.gygsystems.com"> www.gygsystems.com </a></span>
                            </p>
                        </div>
                    </div>

                </div>
            </div>
        </footer>
    </div>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>
